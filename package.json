{
  "name": "@gnosis.pm/cow-sdk",
  "version": "0.0.6",
  "license": "(MIT OR Apache-2.0)",
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "prettier": "^2.5.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist-esm/index.js",
  "jsnext:main": "dist-esm/index.js",
  "types": "dist-esm/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "dist-esm/**/*"
  ],
  "scripts": {
    "build": "npm run build:commonjs && npm run build:esm",
    "build:commonjs": "rimraf dist && ttsc",
    "build:esm": "rimraf dist-esm && ttsc -m esnext -t es2015 --outDir dist-esm",
    "start": "npx ttsc --watch",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.+(ts|json)\"",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "@gnosis.pm/gp-v2-contracts": "^1.1.2",
    "ajv": "^8.8.2",
    "cross-fetch": "^3.1.5",
    "ethers": "^5.5.3",
    "loglevel": "^1.8.0"
  },
  "keywords": [
    "cow",
    "cow-protocol",
    "sdk",
    "crypto",
    "typescript"
  ]
}
