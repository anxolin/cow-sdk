{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "/*": ["src/*"]
    },
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "alwaysStrict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
  }
}
